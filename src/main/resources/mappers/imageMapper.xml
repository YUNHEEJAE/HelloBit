<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.kb141.mapper.ImageMapper">

	<insert id="create">
		insert into tbl_image (sid, filename)
		values(#{sid},#{filename})
	</insert>



	<update id="update" parameterType="ImageVO">

		update tbl_image set filename
		= #{filename} where ino=#{ino}

	</update>

	<delete id="delete">

		delete from tbl_image where ino = #{ino}

	</delete>

	<select id="allList" resultType="ImageVO">

		select * from tbl_image

	</select>


</mapper>